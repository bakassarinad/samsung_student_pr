import java.util.Scanner;
import java.io.*;
public class MyClass {

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 Scanner scan = new Scanner(System.in);
		 int a = scan.nextInt();
		 int result =1;
		 for (int i = 1; i <= a; ++i) {
			 for (int j = 1; j <= a; ++j) {
				 result *= j;
			 }
			
			 
		 }
		 System.out.println(result);
	     
}
	
}
