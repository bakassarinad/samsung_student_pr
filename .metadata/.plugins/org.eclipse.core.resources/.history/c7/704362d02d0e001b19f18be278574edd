import java.util.Scanner;
import java.io.*;
public class MyClass {

	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		int x = scan.nextInt();
		int sum = 0;
		int z;
		int result;
		while (x > 10) {
			z = x % 10;
			result = z;
			result = z * 10;
		}
		
	}
}
